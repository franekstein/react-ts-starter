  },
  "name": "react-ts-starter",
  "repository": {
    "type": "git",
    "url": "git@github.com:franekstein/react-ts-starter"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "build:serve": "cross-env serve -s dist",
    "format": "cross-env prettier . --write",
    "lint": "cross-env eslint . --fix --max-warnings=0",
    "prepare": "husky install",
    "server": "cross-env yarn build && node server/fastify.js",
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "test": "yarn test:unit && yarn test:e2e",
    "test:e2e": "npx playwright test --reporter=list",
    "test:report": "npx playwright show-report",
    "test:unit": "cross-env jest --coverage"
  },
  "version": "1.0.2"
